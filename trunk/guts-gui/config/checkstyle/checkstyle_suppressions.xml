<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Puppy Crawl//DTD Suppressions 1.0//EN"
    "http://www.puppycrawl.com/dtds/suppressions_1_0.dtd">

<suppressions>
  <!-- Remove checks for test files -->
  <suppress checks=".*" files=".*Test\.java$" />

  <!-- Remove checks for binary files -->
  <suppress checks=".*" files=".*\.png$" />
  <suppress checks=".*" files=".*\.jpg$" />
  <suppress checks=".*" files=".*\.gif$" />
  <suppress checks=".*" files="\.keepme$" />

  <!-- Remove checks for binary files -->
  <suppress checks=".*" files=".*\.properties$" />
</suppressions>
 